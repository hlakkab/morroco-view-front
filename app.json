{
  "expo": {
    "name": "morroco-view-front",
    "slug": "morroco-view-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "ma.morrocoview"
    },
    "web": {},
    "assets": [
      "./node_modules/@react-navigation/elements/lib/commonjs/assets"
    ],
    "plugins": [
      "expo-secure-store"
    ]
  }
}
